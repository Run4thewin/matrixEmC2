// SUBSTITUIR POR DOIS FOR
#include <stdio.h>
#include <math.h>
#include<stdlib.h>


int main(void){

    float m[3][3];
    float h;

    for(int i = 0; i < 3; i++){
            for(int j = 0; j < 3; j++){
                printf("Digite o valor da posicao %i %i da matriz ", i, j);
                scanf( "%f", &m[i][j]);
            }
    }




    for(int i = 0; i < 3; i++){
            for(int j = 0; j < 3; j++){
            int c = m[i][j];
                printf( " %f ", c);
            }
            printf("\n");
    }

            printf("\n");


// HOW TO DONT WORK WITH MATRIX
h = 9/((1/m[0][0]) + (1/m[0][1]) + (1/m[0][2]) + (1/m[1][0]) + (1/m[1][1]) + (1/m[1][2]) + (1/m[2][0]) + (1/m[2][1]) + (1/m[2][2]));
printf("O valor: %f",h);

return 0;

}
